package com.springcore.firstspringcore.listCore;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class TestList {
	public static void main(String[] args) {
		ApplicationContext context = new ClassPathXmlApplicationContext("com/springcore/firstspringcore/listCore/list.xml");
		CoreList corelist = (CoreList) context.getBean("corelist");
		System.out.println(corelist);
	}
}
