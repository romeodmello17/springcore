package com.springcore.firstspringcore;

public class TestCore {
	private int id;
	private String name;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public TestCore(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}

	public TestCore() {

	}

	@Override
	public String toString() {
		return "TestCore [id=" + id + ", name=" + name + "]";
	}

}
