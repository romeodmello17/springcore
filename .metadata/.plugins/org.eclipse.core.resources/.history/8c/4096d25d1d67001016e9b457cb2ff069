package com.springcore.firstspringcore.ci;

public class Addition {
	private int a;
	private int b;

	public Addition(int a, int b) {
		this.a = a;
		this.b = b;
		System.out.println("int const");
	}

	public Addition(double a, double b) {
		this.a = (int) a;
		this.b = (int) b;
		System.out.println("double const");
	}

	public void doSum() {
		System.out.println("Sum of a + b : " + (this.a + this.b));
	}
}
